function tau = tau_area(steptime,initialvalue,finalvalue,times,values)
    S = stepinfo(times,values);
    ts = S.SettlingTime;
    t1 = times(find(times <= steptime));
    Vt_ss=(finalvalue-initialvalue)*ganho(steptime,initialvalue,finalvalue,times,values);
    y = Vt_ss-values(find(times <= ts & times >= steptime));
    A0 = trapz(t,y);
    tau = A0/Vt_ss;
end